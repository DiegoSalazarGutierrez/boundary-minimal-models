	pages = {137--261},
	file = {De Sole and Kac - 2006 - Finite vs affine W-algebras.pdf:C\:\\Users\\bashi\\Zotero\\storage\\JVBYI2IM\\De Sole and Kac - 2006 - Finite vs affine W-algebras.pdf:application/pdf},
